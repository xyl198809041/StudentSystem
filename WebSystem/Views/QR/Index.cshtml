
@model DataSystem.DataList
@{
    ViewBag.Title = "班级二维码";
}

<div class="row">
    <div class="col">
    </div>
    <div class="col text-center">
        <ul class="list-group">
            <li class="list-group-item list-group-item-dark">班级</li>
            @for (int i = 0; i < Model.Count; i++)
            {
                <li class="list-group-item list-group-item-action">
                    @Html.ActionLink(Model[i].Name, "Get", "QR", null, new { @class = "" })
                </li>

            }
        </ul>
    </div>
    <div class="col">
    </div>
</div>

